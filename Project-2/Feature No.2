#include <Arduino.h>

void setup() {
  Serial.begin(9600);
}

void loop() {
  if (Serial.available() > 0) {
    String input = Serial.readStringUntil('\n');
    int v1x, v1y, v2x, v2y;

    sscanf(input.c_str(), "ANGLE (%d,%d) (%d,%d)", &v1x, &v1y, &v2x, &v2y);

    float angle = atan2(v1x * v2y - v1y * v2x, v1x * v2x + v1y * v2y) * 180.0 / PI;

    Serial.print("The angle between the two vectors is: ");
    Serial.print(angle);
    Serial.println(" degrees");
  }
}
